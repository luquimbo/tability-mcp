{
  "name": "tability-mcp",
  "version": "1.0.0",
  "description": "MCP server for Tability.app - OKR and goal tracking platform",
  "main": "dist/index.js",
  "module": "dist/smithery.js",
  "types": "dist/index.d.ts",
  "bin": {
    "tability-mcp": "./dist/index.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "dev:http": "tsx src/http-server.ts",
    "start": "node dist/index.js",
    "start:http": "node dist/http-server.js",
    "prepublishOnly": "npm run build",
    "inspector": "npx @modelcontextprotocol/inspector node dist/index.js"
  },
  "keywords": [
    "mcp",
    "tability",
    "okr",
    "goals",
    "model-context-protocol",
    "claude",
    "ai",
    "assistant",
    "key-results",
    "objectives"
  ],
  "author": "Master Me Up",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luquimbo/tability-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/luquimbo/tability-mcp/issues"
  },
  "homepage": "https://github.com/luquimbo/tability-mcp#readme",
  "type": "module",
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.9",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
